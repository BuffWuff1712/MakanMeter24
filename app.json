{
  "expo": {
    "name": "MakanMeter24",
    "slug": "MakanMeter",
    "scheme": "makanmeter",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "$(MakanMeter) needs access to your Camera to take photos."
      },
      "bundleIdentifier": "com.bingchillingz1712.MakanMeter",
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.bingchillingz1712.MakanMeter",
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "$(MakanMeter) needs access to your Camera to take photos."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "$(MakanMeter) needs access to your photo library."
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d5aacd91-8be8-4c92-b250-0ba31e53c9f4"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/d5aacd91-8be8-4c92-b250-0ba31e53c9f4"
    }
  }
}
